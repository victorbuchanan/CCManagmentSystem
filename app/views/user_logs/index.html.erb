<h1>Listing user_logs</h1>

<table>
  <tr>
    <th>Userid</th>
    <th>Transactiondate</th>
    <th>Billamount</th>
    <th>Paid</th>
    <th>Adminid</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @user_logs.each do |user_log| %>
  <tr>
    <td><%= user_log.userid %></td>
    <td><%= user_log.transactionDate %></td>
    <td><%= user_log.billAmount %></td>
    <td><%= user_log.paid %></td>
    <td><%= user_log.adminId %></td>
    <td><%= link_to 'Show', user_log %></td>
    <td><%= link_to 'Edit', edit_user_log_path(user_log) %></td>
    <td><%= link_to 'Destroy', user_log, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

